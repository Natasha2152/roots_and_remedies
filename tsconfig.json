{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "incremental": true,
    "outDir": "dist",
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "./**/*.ts",
    "app/api/herbalmedicine",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
